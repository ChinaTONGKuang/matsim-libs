<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v2.dtd">
<config>
	<module name="postgresExporter" >
		<!-- xml-File containing database host, database, user, password, port -->
		<param name="dbParamFile" value="./dbParam.xml" />
		
		<!-- Possible values: failIfRunIdExists, overwriteExistingRunId-->
		<param name="overwriteRun" value="overwriteExistingRunId" />

		<!-- directory of where sql analyzer queries are stored-->
		<param name="sqlAnalyzerDir" value="./analyzerQueries" />
			
		<parameterset type="initialData" >
			<!-- Possible values: importOverwrite, importIfNotExist, NoImport-->
			<param name="importMode" value="importIfNotExist" />

			<!-- shp-File containing geo data of aggregation areas such as counties, communities, hexagons etc... -->
			<param name="aggregationAreasFile" value="./aggregationAreas.shp" />
			<param name="aggregationAreasCrs" value="" />
			<param name="databaseCrs" value="" />

		</parameterset>
	</module>
	
</config>
